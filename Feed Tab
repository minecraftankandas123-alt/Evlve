import javax.swing.*;
import java.awt.*;

public class FeedTab extends JPanel {
    private EvolveApp app;

    public FeedTab(EvolveApp app) {
        this.app = app;
        setLayout(new BorderLayout());
        JLabel label = new JLabel("Feed: Posts, Likes, Comments", SwingConstants.CENTER);
        label.setFont(new Font("Arial", Font.BOLD, 24));
        add(label, BorderLayout.NORTH);

        // Placeholder: Add your feed logic here (posts list, like, comment, etc)
        add(new JLabel("Feed goes here...", SwingConstants.CENTER), BorderLayout.CENTER);
    }
}
